{
  "name": "@whitespace/storybook-addon-html",
  "version": "3.0.0",
  "description": "A Storybook addon that extracts and displays compiled syntax-highlighted HTML",
  "main": "out/preset.js",
  "files": [
    "out/**/*",
    "README.md",
    "*.js"
  ],
  "keywords": [
    "addon",
    "storybook",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/whitespace-se/storybook-addon-html.git"
  },
  "scripts": {
    "build": "yarn clean && babel -d ./out ./src",
    "build:watch": "babel --watch -d ./out ./src",
    "clean": "rm -rf ./out",
    "prepare": "yarn build",
    "prepublishOnly": "cp ../README.md ."
  },
  "author": "Whitespace",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@storybook/addons": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "babel-eslint": "^10.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.1.10",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.12.5",
    "@babel/parser": "^7.12.7",
    "@storybook/api": "^6.1.10",
    "@storybook/components": "^6.1.10",
    "prettier": "^2.2.1",
    "react-syntax-highlighter": "^15.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
